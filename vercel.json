{
    "version": 2,
    "builds": [
        {
            "src": "api/vercel.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["dist/**", "pages/**", ".well-known/**"]
            }
        },
        {
            "src": "pages/**",
            "use": "@vercel/static"
        },
        {
            "src": ".well-known/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/authentication/(.*)",
            "dest": "api/vercel.js"
        },
        {
            "src": "/",
            "dest": "pages/home.html"
        }
    ]
}
