{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["dist/**", "pages/**", ".well-known/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/authentication/(.*)",
            "dest": "dist/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}
