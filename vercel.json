{
    "version": 2,
    "installCommand": "npm install",
    "buildCommand": "npx tsc && node dest/index",
    "outputDirectory": "dest",
    "builds": [
        {
            "src": "dest/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dest/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dest/index.js"
        }
    ]
}
